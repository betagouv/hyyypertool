{
  "name": "hyyypertool",
  "module": "src/bootstrap.ts",
  "type": "module",
  "version": "2023.11.15",
  "dependencies": {
    "@gouvfr/dsfr": "1.10.2",
    "@hono/zod-validator": "^0.1.11",
    "@lit-labs/motion": "^1.0.6",
    "@prisma/client": "5.6.0",
    "animate.css": "4.1.1",
    "await-to-js": "3.0.0",
    "hono": "^3.10.2",
    "htmx.org": "1.9.9",
    "hyperscript.org": "0.9.12",
    "lit": "^3.1.0",
    "query-string": "^8.1.0",
    "tailwind-variants": "^0.1.18",
    "ts-pattern": "^5.0.5",
    "zod": "^3.22.4"
  },
  "devDependencies": {
    "@tailwindcss/typography": "0.5.10",
    "@tsconfig/bun": "1.0.1",
    "bun-types": "1.0.13",
    "moncomptepro": "betagouv/moncomptepro",
    "prettier": "3.1.0",
    "prettier-plugin-tailwindcss": "0.5.7",
    "prisma": "5.6.0",
    "tailwindcss": "3.3.5",
    "typescript": "5.3.2"
  },
  "scripts": {
    "build:prisma": "bun x prisma generate",
    "build:tailwind": "bun x tailwindcss -i ./src/www/styles.css -o ./public/tailwind/styles.css",
    "build": "bun x npm-run-all --serial build:**",
    "dev:tailwind": "bun run build:tailwind --watch",
    "dev": "bun --hot src/bootstrap.ts",
    "start": "bun src/bootstrap.ts",
    "studio": "bun x prisma studio",
    "test:type-check": "bun x tsc --noEmit",
    "test": "bun x npm-run-all --serial test:**"
  },
  "prettier": {
    "plugins": [
      "prettier-plugin-tailwindcss"
    ],
    "tailwindFunctions": [
      "tv"
    ]
  }
}
