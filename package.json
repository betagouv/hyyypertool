{
  "name": "hyyypertool",
  "module": "src/bootstrap.ts",
  "type": "module",
  "version": "2023.11.15",
  "dependencies": {
    "@elysiajs/html": "0.7.3",
    "@elysiajs/static": "0.7.1",
    "@gouvfr/dsfr": "1.10.2",
    "@kitajs/html": "3.0.10",
    "@kitajs/ts-html-plugin": "1.3.3",
    "@lit-labs/motion": "^1.0.6",
    "@prisma/client": "5.6.0",
    "animate.css": "4.1.1",
    "await-to-js": "3.0.0",
    "elysia": "0.7.28",
    "elysia-autoroutes": "0.4.0",
    "elysia-compression": "0.0.6",
    "htmx.org": "1.9.8",
    "hyperscript.org": "0.9.12",
    "lit": "^3.1.0"
  },
  "devDependencies": {
    "@tailwindcss/typography": "0.5.10",
    "@tsconfig/bun": "1.0.1",
    "bun-types": "1.0.13",
    "prettier": "3.1.0",
    "prettier-plugin-tailwindcss": "0.5.7",
    "prisma": "5.6.0",
    "tailwindcss": "3.3.5",
    "typescript": "5.2.2"
  },
  "scripts": {
    "build:prisma": "bun x prisma generate",
    "build:tailwind": "bun x tailwindcss -i ./src/www/styles.css -o ./public/tailwind/styles.css",
    "build": "bun x npm-run-all --serial build:**",
    "dev:tailwind": "bun run build:tailwind --watch",
    "dev": "bun --hot src/bootstrap.ts",
    "start": "bun src/bootstrap.ts",
    "test:type-check": "bun x tsc --noEmit",
    "test:xss-scan": "xss-scan",
    "test": "bun x npm-run-all --serial test:**"
  },
  "prettier": {
    "plugins": [
      "prettier-plugin-tailwindcss"
    ],
    "tailwindFunctions": [
      "tv"
    ]
  }
}
